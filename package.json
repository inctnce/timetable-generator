{
  "name": "timetable-generator",
  "main": "public/main.js",
  "homepage": "./",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^2.13.2",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.4",
    "@mui/x-date-pickers": "^5.0.0-alpha.2",
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "concurrently": "^7.2.1",
    "date-fns": "^2.28.0",
    "gh-pages": "^4.0.0",
    "npm": "^8.9.0",
    "papaparse": "^5.3.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux-persist": "^6.0.0",
    "serve": "^13.0.2",
    "transition-group": "^0.0.2",
    "typescript": "^4.6.4",
    "web-vitals": "^2.1.4"
  },
  "engines": {
    "npm": "7.0.0",
    "node": "16.0.0"
  },
  "scripts": {
    "start": "serve -s build",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"npm run electron:start\"",
    "electron:build": "npm run build && electron-builder --config ./electron.json -mwl -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on tcp:3000 && electron .",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron/remote": "^2.0.8",
    "@types/papaparse": "^5.3.2",
    "@types/remote-redux-devtools": "^0.5.5",
    "cross-env": "^7.0.3",
    "electron": "^19.0.1",
    "electron-builder": "^23.0.3",
    "remote-redux-devtools": "^0.5.16",
    "wait-on": "^6.0.1"
  }
}
